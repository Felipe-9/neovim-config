    /* ====== interpolação e aproximação polinomial ===== */
    "polinomio de lagrange": {
        "description": "",
        "prefix": "\\formula-polinomio-lagrange",
        "body": [
            "p_{n\\,(x)}=\\sum_{i=0}^{n}{y_i\\,l_{i\\,(x)}}",
            ";${1:&}\\\\\\\\${1:&}",
            "l_{i\\,(x)}",
            "=\\varphi_{i\,(x)}",
            "=\\prod_{j=0}^{i-1}{\\frac{x-x_j}{x_i-x_j}}",
            "\\,\\prod_{j=i+1}^{n}{\\frac{x-x_j}{x_i-x_j}}",
        ]
    },
    "polinómio de newton com diferenças divididas": {
        "description": "",
        "prefix": "\\formula-polinomio-newton-com-diff-div",
        "body": [
            "p_{n\\,(x)}",
            "= f_0",
            "+ \\sum_{i=0}^{n-1}{",
            "    \\left(\\prod_{j=0}^{i}{x-x_j}\\right)",
            "    \\,f_{[x_0,\\dots,x_{i+1}]}",
            "}"
        ]
    },
    "erro de interpolação": {
        "description": "",
        "prefix": "\\formula-erro-interpolacao",
        "body": [
            "f_{x^*}-p_{n\\,(x^*)}",
            "= \\frac{f^{x+1}_{(\\xi)}}{(n+1)!}",
            "\\prod_{i=0}^{n}{x^*-x_i}",
            "\\\\\\\\\\xi\\in\\myrange*{",
            "    \\min(x_0,x_1,\\dots,x_n),",
            "    \\max(x_0,x_1,\\dots,x_n)",
            "}"
        ]
    },
    "interpolação por splines polinomiais cúbicos": {
        "description": "expressão de spline polinomial cúbico, interpolador de f em x_0,x_1,...,x_n no intervalo [x_i,x_{i+1}]",
        "prefix": "\\formula-interp-spline-poli-cubico",
        "body": [
            "s_{i\\,(x)}",
            "= \\left(",
            "    \\begin{aligned}",
            "        - &",
            "            \\frac{(x-x_{i+1})^3}{6\\,h_i}\\,m_i",
            "        &+\\\\\\\\+&",
            "            \\frac{(x-x_i)^3}{6\\,h_i}\\,m_{i+1}",
            "        &+\\\\\\\\+&",
            "            \\left(",
            "                f_i",
            "                - \\frac{h_i^2}{6}\\,m_i",
            "            \\right)",
            "            \\,\\frac{x_{i+1}-x}{h_i}",
            "        &+\\\\\\\\+&",
            "            \\left(",
            "                f_{i+1}",
            "                - \\frac{h_i^2}{6}\\,m_{i+1}",
            "            \\right)",
            "            \\,\\frac{x-x_i}{h_i}",
            "        &",
            "    \\end{aligned}",
            "\\right)"
            ";${1:&}\\\\\\\\${1:&}",
            "h_i=x_{i+1}-x_i",
            ",\\quad i=0,1,\\dots,n-1",
            ";${1:&}\\\\\\\\${1:&}",
            "m_i=\\odv[order=2]{s_{(x_i)}}{x}",
            ",\\quad i=0,1,\\dots,n",
        ]
    },
    "sistema de equações para det spline cubico interpolador de f": {
        "description": "sistema de equações para determinação de um spline cúbico interpolador de f nos pontos x_0,x_1,...,x_n",
        "prefix": "\\formula-sistema-eq-spline-cubico-interp",
        "body": [
            "h_{i-1}\\,m_{i-1}",
            "+ 2\,(h_{i-1}+h_i)\\,m_i",
            "+ h_i\\,m_{i+1}",
            "= 6\\left(",
            "    \\frac{f_{i+1}-f_{i}}{h_i}",
            "    -\\frac{f_{i}-f_{i-1}}{h_{i-1}}",
            "\\right)",
            ",\\quad i\\in\\myrange{1,n-1}",
        ]
    },
    "sistema de equações para determinação do spline cúbico completo": {
        "description": "adicionar às equações [e] as equações",
        "prefix": "\\formula-sistema-eq-spline-cubico-completo",
        "body": [
            "2\\,h_0\\,m_0 + h_0\\,m_1",
            "= 6\\,\\left(",
            "    \\frac{f_1-f_0}{h_0}-f'_{0}",
            "\\right)",
            "&\\\\\\\\&",
            "h_{n-1}\\,m_{n-1} + 2\\,h_{n-1}\\,m_n",
            "= 6\\,\\left(",
            "    f'_{0}-\\frac{f_n-f_{n-1}}{h_{n-1}}",
            "\\right)",
        ]
    },
    "sistema de equações para determinação do spline cúbico natural": {
        "description": "adicionar às equações [e] as equações",
        "prefix": "\\formula-sistema-eq-spline-cubico-natural",
        "body": "m_0=m_n=0"
    },
    "metodo dos mínimos quadrados": {
        "description": "função polinomial que melhor aproxima os dados {(x_0,y_0),...,(x_n,y_n)} em relacao à base {1,x,...,x^m}",
        "prefix": "\\formula-metodo-minimos-quadrados",
        "body": [
            "p_{(x)} = \\sum_{i=0}^{m}{\\alpha_i\\,x^i}",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "\\begin{cases}",
            "    \\sum_{i=0}^{m}{\\left(",
            "        \\alpha_i\\,\\sum_{j=0}^{n}{x_{j}^{i+k}}",
            "    \\right)}",
            "    = \\sum_{i=0}^{n}{x_i^k\\,y_i}",
            "    \\\\\\\\",
            "    k\\in\\myrange{0,m}",
            "\\end{cases}",
        ]
    },
    /* =============== integração numérica ============== */
    // formulas de newton-cotes (fechadas)
    "formulas de newton-cotes (fechadas) 0": {
        "description": "regra do ponto médio",
        "prefix": "\\formula-newton-cotes-0-regra-ponto-medio",
        "body": [
            "% integração",
            "\\int_{a=x_0}^{b=x_1}{f_{(x)}\\,\\odif{x}}",
            "\\approx h\\,f_{\\left(\\frac{x_0+x_1}{2}\\right)}",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "% erro de quadratura",
            "\\frac{h^3}{24}\\,\\odv[order=2]{f_{(\\xi)}}{x}",
            ";\\quad\\xi\\in\\myrange*{x_0,x_1}"
        ]
    },
    "formulas de newton-cotes (fechadas) 1": {
        "description": "regra dos trapézios",
        "prefix": "\\formula-newton-cotes-1-regra-trapezios",
        "body": [
            "% integração",
            "\\int_{a=x_0}^{b=x_1}{f_{(x)}\\,\\odif{x}}",
            "\\approx \\frac{h}{2}\\,(f_0+f_1)",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "% erro de quadratura",
            "-\\frac{h^3}{12}\\,\\odv[order=2]{f_{(\\xi)}}{x}",
            ";\\quad\\xi\\in\\myrange*{x_0,x_1}"
        ]
    },
    "formulas de newton-cotes (fechadas) 2": {
        "description": "regra de simpson simples",
        "prefix": [
            "\\formula-newton-cotes-2-regra-simpson-simples",
            "\\formula-regra-simpson-simples"
        ],
        "body": [
            "% integração",
            "\\int_{a=x_0}^{b=x_2}{f_{(x)}\\,\\odif{x}}",
            "=i",
            "\\approx \\hat{i}",
            "=\\frac{h}{3}\\,(f_0+4\\,f_1+f_2)",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "% erro de quadratura",
            "\\varepsilon_i",
            "=i-\\hat{i}_s",
            "= -\\frac{h^5}{90}\\,f^{(4)}(\xi)",
            ";\\quad\\xi\\in\\myrange*{x_0,x_2}"
        ]
    },
    "regra de simpson composta": {
        "description": [
            "regra de simpson composta",
            "\ngrau de precisão: 3",
            "\nσ ∈ ]a,b[",
            "\nh = (b-a)/2n"
        ],
        "prefix": "\\formula-regra-simpson-composta",
        "body": [
            "% integração",
            "\\int_{a=x_0}^{b=x_2}{f_{(x)}\\,\\odif{x}}",
            "=i",
            "\\approx \\hat{i}",
            "= \\frac{h}{3}\\left(",
            "    f(x_0)",
            "    + 4\\,\\sum_{j=1}^{n  }{f(x_{(2\\,j-1)})}",
            "    + 2\\,\\sum_{j=1}^{n-1}{f(x_{(2\\,j  )})}",
            "    + f(x_{2\\,n})",
            "\\right)",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "% erro de quadratura",
            "\\varepsilon_i",
            "= i-\\hat{i}_s",
            "= -n\\,\\frac{h^5}{90}\\,f^{(4)}(\\sigma)",
            ";\\quad\\sigma\\in\\myrange*{x_0,x_2}"
        ]
    },
    "método de integração de gauss": {
        "description": "",
        "prefix": "\\formula-metodo-integracao-gauss",
        "body": [
            "\\int_a^b{f_{(x)}\\,\\odif{x}}",
            "= \\frac{b-a}{2}",
            "\\,\\int_{-1}^{1}{f_{\\left(",
            "    \\frac{b-a}{2}\\,y",
            "    + \\frac{b+a}{2}",
            "\\right)}\\,\\odif{y}}",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "f\\in c^{(2\\,n)}_{(\\myrange{-1,1})}",
            // "\\implies ${1:&}\\\\\\\\${1:&}",
            "\\implies",
            "i-\\hat{i}",
            "= \\frac{"
            "    2^{2\\,n+1}\\,(n!)^4",
            "}{",
            "    (2\\,n+1)\\,((2\\,n)!)^3",
            "}",
            "\\,\\odv[order={2\\,n}]{f_{(\\theta)}}{x}",
            ",\\quad\\theta\\in\\myrange{-1,1}"
        ]
    },
    "cn a convergencia": {
        "description": "convergencia de uma sucessão x_i=f(x_(i-1)) a partir de um x_0 para α",
        "prefix": "\\formula-cna-convergencia-sucessao",
        "body": [
            "\\text{condições de conversão}&\\\\\\\\&",
            "\\begin{cases}",
            "    f(x) \\text{ é contínua em } i",
            "    \\\\\\\\",
            "    f(x)\in i,\\forall x\\in i",
            "    \\\\\\\\",
            "    \\myvert{f'(x)}\\leq\\myvert{f'(\\alpha)}",
            "\\end{cases}"
        ]
    },
    /* -------------------------------------------------- */
