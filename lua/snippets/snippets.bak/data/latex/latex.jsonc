    "indexer": {
        "description": "input some files using luacode",
        "prefix": "\\indexer",
        "body": [
            "% print files",
            "\\begin{luacode*}",
            "",
            "    -- Files Directory",
            "    dir = \"\"",
            "",
            "",
            "    -- Prefix and sufix",
            "    prefix = \"\"",
            "    sufix  = \"\"",
            "",
            "    -- Files",
            "    files = {",
            "        -- 1, --",
            "    }",
            "",
            "    -- Print some files",
            "    index = {1}",
            "",
            "    -- Loop",
            "    -- for v,i in ipairs(index) -- Generic some",
            "    -- for i,v in ipairs(files) -- Generic all",
            "",
            "    for i,v in ipairs(files) -- Generic all",
            "    do",
            "        tex.print(string.split(table.concat{",
            "        [[",
            "            \\input{]], dir, prefix, files[i], sufix, [[}\\newpage",
            "        ]]",
            "        },\"\\n\"))",
            "    end",
            "",
            "\\end{luacode*}",
        ]
    },
