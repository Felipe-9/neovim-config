    /* -------------------------------------------------- */
    "parabolas": {
        "description": [
            "parametrizando parabolas",
        ],
        "prefix": "\\formula-parabolas",
        "body": [
            "y=\\alpha\\,x^2+\\beta\\,x+\\gamma",
            "\\implies &\\\\\\\\&",
            "\\implies",
            "\\begin{cases}",
            "    a = (4\\,\\alpha)^{-1}",
            "    \\\\\\\\",
            "    x_0",
            "    = -\\beta\\,2\\,a",
            "    = -\\beta/2\\,\\alpha",
            "    \\\\\\\\",
            "    y_0",
            "    = \\gamma-x_0^2/4\\,a",
            "    = \\gamma-\\beta/4\\,\\alpha",
            "\\end{cases}"
        ]
    },
    "elipses": {
        "description": "caracterizando elipses",
        "prefix": "\\formua-elipses",
        "body": [
            "\\frac{(x-x_0)^2}{r_1^2}",
            "+ \\frac{(y-y_0)^2}{r_2^2}",
            "= 1",
            "&\\\\\\\\&",
            "\\begin{cases}",
            "    c = \\myvert{\\vec{p_0\\,f}}",
            "    = \\sqrt{r_1^2+r_2^2}",
            "    \\\\\\\\",
            "    f_1\\in\\mathbb{r}^2:f_1=(x_0-c,y_0)",
            "    \\\\\\\\",
            "    f_1\\in\\mathbb{r}^2:f_1=(x_0-c,y_0)",
            "\\end{cases}"
        ]
    },
    "hiperboles": {
        "description": "caracterizando hiperboles",
        "prefix": "\\formula-hiperboles",
        "body": [
            "\\left(",
            "    \\frac{x-x_0}{r_1}",
            "\\right)",
            "- \\left(",
            "    \\frac{y-y_0}{r_2}",
            "\\right)",
            "= 1",
            "&\\\\\\\\&",
            "\\begin{cases}",
            "    c = \\myvert{\\vec{p_0\\,f}}",
            "    = \\sqrt{r_1^2+r_2^2}",
            "    \\\\\\\\",
            "    v=(x_0\\pm r_1,y_0)",
            "    \\\\\\\\",
            "    f=(x_0\\pm c,y_0)",
            "    \\\\\\\\",
            "    a = \\left{",
            "        (x,y):(y-y_0)",
            "        = \\pm\\frac{r_2}{r_1}",
            "        \\,(x-x_0)",
            "    \\right}",
            "\\end{cases}"
        ]
    },
    "plano tangente a f": {
        "description": "formula para plano tangente a uma superficie f genérica no ponto genérico (x0,y0,z0)",
        "prefix": "\\formula-plano-tangente",
        "body": [
            "\\left(",
            "    \\begin{aligned}",
            "        &",
            "               \\pdv{f}{x}(x_0)\\,(x-x_0)",
            "        &+\\\\\\\\+& \\pdv{f}{y}(y_0)\\,(y-y_0)",
            "        &+\\\\\\\\+& \\pdv{f}{z}(z_0)\\,(z-z_0)",
            "        &",
            "    \\end{aligned}",
            "\\right)",
            "= 0",
        ]
    },
    "limite segundo cauchy": { 
        "description": "definição de limite segundo cauchy",
        "prefix": "\\formula-limite-cauchy",
        "body": [
            "\\forall\\,\\delta>0\\,",
            "\\exists\\,\\varepsilon>0:",
            "\\left(",
            "    \\left(",
            "        \\forall x\\in \\dominio",
            "        \\land",
            "        \\myvert{x-a}<\\varepsilon",
            "    \\right)",
            "    \\implies",
            "    \\myvert{",
            "        f(x)-b",
            "    }<\\delta",
            "\\right)",
        ]
    },
    "jacobiana(2x2)": {
        "description": "jacobiana 2x2",
        "prefix": "\\formula-jacobiana-2x2",
        "body": [
            "\\begin{bmatrix}",
            "    \\pdv{x}{u} & \\pdv{x}{v}",
            "    \\\\\\\\",
            "    \\pdv{y}{u} & \\pdv{y}{v}",
            "\\end{bmatrix}_{(x,y)}"
        ]
    },
    "matriz hessiana 2x2": {
        "description": [
            "matriz hessiana 2x2\nmáximos e minimos\n",
            "det h:\n>0: crítico local\n=0: indeterminável\n<0: ponto de sela\n",
            "det h>0 and d^2f/dx^2:\n<0: mínimo local\n>0: máximo local"
        ],
        "prefix": [
            "\\formula-matrix-hessiana-2",
            "\\formula-matriz-hessiana-2",
        ],
        "body": [
            "\\det h_f=\\begin{vmatrix}",
            "      \\pdv{f}{x,x}",
            "    & \\pdv{f}{x,y}",
            "    \\\\\\\\\\pdv{f}{y,x}",
            "    & \\pdv{f}{y,y}",
            "\\end{vmatrix}"
        ]
    },
    "parametrização coordenadas esfericas": {
        "description": "parametrização coordenadas esfericas",
        "prefix": "\\formula-coordenadas-esfericas",
        "body": [
            "\\begin{cases}",
