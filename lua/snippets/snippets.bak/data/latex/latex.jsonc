            "    do",
            "        tex.print(string.split(table.concat{",
            "        [[",
            "            \\subfile{]], dir, prefix, string.format(\"%d\", i), sufix, [[}\\newpage",
            "        ]]",
            "        },\"\\n\"))",
            "    end",
