        "body": [
            "\\frac",
            "{\\mathscr{d}_{${1:a},${2:b},${5:t_2}${7:\\,\\unit{\\kelvin}},${6:p_2}${8:\\,\\unit{\\atm}}}}",
            "{\\mathscr{d}_{${1:a},${2:b},${3:t_1}${7:\\,\\unit{\\kelvin}},${4:p_1}${8:\\,\\unit{\\atm}}}}",
            "= ",
            "\\,\\frac{${4:p_1}}{${6:p_2}}",
