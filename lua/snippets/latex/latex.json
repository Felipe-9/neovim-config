    /* ====== Interpolação e Aproximação Polinomial ===== */
    "Polinomio de Lagrange": {
        "description": "",
        "prefix": "\\formula-polinomio-lagrange",
        "body": [
            "p_{n\\,(x)}=\\sum_{i=0}^{n}{y_i\\,L_{i\\,(x)}}",
            ";${1:&}\\\\\\\\${1:&}",
            "L_{i\\,(x)}",
            "=\\varphi_{i\,(x)}",
            "=\\prod_{j=0}^{i-1}{\\frac{x-x_j}{x_i-x_j}}",
            "\\,\\prod_{j=i+1}^{n}{\\frac{x-x_j}{x_i-x_j}}",
        ]
    },
    "Polinómio de Newton com diferenças divididas": {
        "description": "",
        "prefix": "\\formula-polinomio-newton-com-diff-div",
        "body": [
            "p_{n\\,(x)}",
            "= f_0",
            "+ \\sum_{i=0}^{n-1}{",
            "    \\left(\\prod_{j=0}^{i}{x-x_j}\\right)",
            "    \\,f_{[x_0,\\dots,x_{i+1}]}",
            "}"
        ]
    },
    "Erro de interpolação": {
        "description": "",
        "prefix": "\\formula-erro-interpolacao",
        "body": [
            "f_{x^*}-p_{n\\,(x^*)}",
            "= \\frac{f^{x+1}_{(\\xi)}}{(n+1)!}",
            "\\prod_{i=0}^{n}{x^*-x_i}",
            "\\\\\\\\\\xi\\in\\myrange*{",
            "    \\min(x_0,x_1,\\dots,x_n),",
            "    \\max(x_0,x_1,\\dots,x_n)",
            "}"
        ]
    },
    "Interpolação por splines polinomiais cúbicos": {
        "description": "Expressão de spline polinomial cúbico, interpolador de f em x_0,x_1,...,x_n no intervalo [x_i,x_{i+1}]",
        "prefix": "\\formula-interp-spline-poli-cubico",
        "body": [
            "S_{i\\,(x)}",
            "= \\left(",
            "    \\begin{aligned}",
            "        - &",
            "            \\frac{(x-x_{i+1})^3}{6\\,h_i}\\,m_i",
            "        &+\\\\\\\\+&",
            "            \\frac{(x-x_i)^3}{6\\,h_i}\\,m_{i+1}",
            "        &+\\\\\\\\+&",
            "            \\left(",
            "                f_i",
            "                - \\frac{h_i^2}{6}\\,m_i",
            "            \\right)",
            "            \\,\\frac{x_{i+1}-x}{h_i}",
            "        &+\\\\\\\\+&",
            "            \\left(",
            "                f_{i+1}",
            "                - \\frac{h_i^2}{6}\\,m_{i+1}",
            "            \\right)",
            "            \\,\\frac{x-x_i}{h_i}",
            "        &",
            "    \\end{aligned}",
            "\\right)"
            ";${1:&}\\\\\\\\${1:&}",
            "h_i=x_{i+1}-x_i",
            ",\\quad i=0,1,\\dots,n-1",
            ";${1:&}\\\\\\\\${1:&}",
            "m_i=\\odv[order=2]{S_{(x_i)}}{x}",
            ",\\quad i=0,1,\\dots,n",
        ]
    },
    "Sistema de equações para det spline cubico interpolador de f": {
        "description": "Sistema de equações para determinação de um spline cúbico interpolador de f nos pontos x_0,x_1,...,x_n",
        "prefix": "\\formula-sistema-eq-spline-cubico-interp",
        "body": [
            "h_{i-1}\\,m_{i-1}",
            "+ 2\,(h_{i-1}+h_i)\\,m_i",
            "+ h_i\\,m_{i+1}",
            "= 6\\left(",
            "    \\frac{f_{i+1}-f_{i}}{h_i}",
            "    -\\frac{f_{i}-f_{i-1}}{h_{i-1}}",
            "\\right)",
            ",\\quad i\\in\\myrange{1,n-1}",
        ]
    },
    "Sistema de equações para determinação do spline cúbico completo": {
        "description": "Adicionar às equações [E] as equações",
        "prefix": "\\formula-sistema-eq-spline-cubico-completo",
        "body": [
            "2\\,h_0\\,m_0 + h_0\\,m_1",
            "= 6\\,\\left(",
            "    \\frac{f_1-f_0}{h_0}-f'_{0}",
            "\\right)",
            "&\\\\\\\\&",
            "h_{n-1}\\,m_{n-1} + 2\\,h_{n-1}\\,m_n",
            "= 6\\,\\left(",
            "    f'_{0}-\\frac{f_n-f_{n-1}}{h_{n-1}}",
            "\\right)",
        ]
    },
    "Sistema de equações para determinação do spline cúbico completo": {
        "description": "Adicionar às equações [E] as equações",
        "prefix": "\\formula-sistema-eq-spline-cubico-natural",
        "body": "m_0=m_n=0"
    },
    "Metodo dos mínimos quadrados": {
        "description": "Função polinomial que melhor aproxima os dados {(x_0,y_0),...,(x_n,y_n)} em relacao à base {1,x,...,x^m}",
        "prefix": "\\formula-metodo-minimos-quadrados",
        "body": [
            "p_{(x)} = \\sum_{i=0}^{m}{\\alpha_i\\,x^i}",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "\\begin{cases}",
            "    \\sum_{i=0}^{m}{\\left(",
            "        \\alpha_i\\,\\sum_{j=0}^{n}{x_{j}^{i+k}}",
            "    \\right)}",
            "    = \\sum_{i=0}^{n}{x_i^k\\,y_i}",
            "    \\\\\\\\",
            "    k\\in\\myrange{0,m}",
            "\\end{cases}",
        ]
    },
    /* =============== Integração Numérica ============== */
    // Formulas de Newton-Cotes (fechadas)
    "Formulas de Newton-Cotes (fechadas) 0": {
        "description": "Regra do ponto médio",
        "prefix": "\\formula-newton-cotes-0-regra-ponto-medio",
        "body": [
            "% Integração",
            "\\int_{a=x_0}^{b=x_1}{f_{(x)}\\,\\odif{x}}",
            "\\approx h\\,f_{\\left(\\frac{x_0+x_1}{2}\\right)}",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "% Erro de quadratura",
            "\\frac{h^3}{24}\\,\\odv[order=2]{f_{(\\xi)}}{x}",
            ";\\quad\\xi\\in\\myrange*{x_0,x_1}"
        ]
    },
    "Formulas de Newton-Cotes (fechadas) 1": {
        "description": "Regra dos trapézios",
        "prefix": "\\formula-newton-cotes-1-regra-trapezios",
        "body": [
            "% Integração",
            "\\int_{a=x_0}^{b=x_1}{f_{(x)}\\,\\odif{x}}",
            "\\approx \\frac{h}{2}\\,(f_0+f_1)",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "% Erro de quadratura",
            "-\\frac{h^3}{12}\\,\\odv[order=2]{f_{(\\xi)}}{x}",
            ";\\quad\\xi\\in\\myrange*{x_0,x_1}"
        ]
    },
    "Formulas de Newton-Cotes (fechadas) 2": {
        "description": "Regra de Simpson Simples",
        "prefix": [
            "\\formula-newton-cotes-2-regra-simpson-simples",
            "\\formula-regra-simpson-simples"
        ],
        "body": [
            "% Integração",
            "\\int_{a=x_0}^{b=x_2}{f_{(x)}\\,\\odif{x}}",
            "=I",
            "\\approx \\hat{I}",
            "=\\frac{h}{3}\\,(f_0+4\\,f_1+f_2)",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "% Erro de quadratura",
            "\\varepsilon_I",
            "=I-\\hat{I}_S",
            "= -\\frac{h^5}{90}\\,f^{(4)}(\xi)",
            ";\\quad\\xi\\in\\myrange*{x_0,x_2}"
        ]
    },
    "Regra de Simpson Composta": {
        "description": [
            "Regra de Simpson Composta",
            "\nGrau de precisão: 3",
            "\nσ ∈ ]a,b[",
            "\nh = (b-a)/2n"
        ],
        "prefix": "\\formula-regra-simpson-composta",
        "body": [
            "% Integração",
            "\\int_{a=x_0}^{b=x_2}{f_{(x)}\\,\\odif{x}}",
            "=I",
            "\\approx \\hat{I}",
            "= \\frac{h}{3}\\left(",
            "    f(x_0)",
            "    + 4\\,\\sum_{j=1}^{n  }{f(x_{(2\\,j-1)})}",
            "    + 2\\,\\sum_{j=1}^{n-1}{f(x_{(2\\,j  )})}",
            "    + f(x_{2\\,n})",
            "\\right)",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "% Erro de quadratura",
            "\\varepsilon_I",
            "= I-\\hat{I}_S",
            "= -n\\,\\frac{h^5}{90}\\,f^{(4)}(\\sigma)",
            ";\\quad\\sigma\\in\\myrange*{x_0,x_2}"
        ]
    },
    "Método de integração de Gauss": {
        "description": "",
        "prefix": "\\formula-metodo-integracao-gauss",
        "body": [
            "\\int_a^b{f_{(x)}\\,\\odif{x}}",
            "= \\frac{b-a}{2}",
            "\\,\\int_{-1}^{1}{f_{\\left(",
            "    \\frac{b-a}{2}\\,y",
            "    + \\frac{b+a}{2}",
            "\\right)}\\,\\odif{y}}",
            "${1:&}\\\\\\\\[3ex]${1:&}",
            "f\\in C^{(2\\,n)}_{(\\myrange{-1,1})}",
            // "\\implies ${1:&}\\\\\\\\${1:&}",
            "\\implies",
            "I-\\hat{I}",
            "= \\frac{"
            "    2^{2\\,n+1}\\,(n!)^4",
            "}{",
            "    (2\\,n+1)\\,((2\\,n)!)^3",
            "}",
            "\\,\\odv[order={2\\,n}]{f_{(\\theta)}}{x}",
            ",\\quad\\theta\\in\\myrange{-1,1}"
        ]
    },
    "CN A Convergencia": {
        "description": "Convergencia de uma sucessão x_i=f(x_(i-1)) a partir de um x_0 para α",
        "prefix": "\\formula-cna-convergencia-sucessao",
        "body": [
            "\\text{Condições de conversão}&\\\\\\\\&",
            "\\begin{cases}",
            "    f(x) \\text{ é contínua em } I",
            "    \\\\\\\\",
            "    f(x)\in I,\\forall x\\in I",
            "    \\\\\\\\",
            "    \\myvert{f'(x)}\\leq\\myvert{f'(\\alpha)}",
            "\\end{cases}"
        ]
    },
    /* -------------------------------------------------- */
    /*                        AM 2C                       */
    /* -------------------------------------------------- */
    "Parabolas": {
        "description": [
            "Parametrizando parabolas",
        ],
        "prefix": "\\formula-parabolas",
        "body": [
            "y=\\alpha\\,x^2+\\beta\\,x+\\gamma",
            "\\implies &\\\\\\\\&",
            "\\implies",
            "\\begin{cases}",
            "    a = (4\\,\\alpha)^{-1}",
            "    \\\\\\\\",
            "    x_0",
            "    = -\\beta\\,2\\,a",
            "    = -\\beta/2\\,\\alpha",
            "    \\\\\\\\",
            "    y_0",
            "    = \\gamma-x_0^2/4\\,a",
            "    = \\gamma-\\beta/4\\,\\alpha",
            "\\end{cases}"
        ]
    },
    "Elipses": {
        "description": "Caracterizando elipses",
        "prefix": "\\formua-elipses",
        "body": [
            "\\frac{(x-x_0)^2}{r_1^2}",
            "+ \\frac{(y-y_0)^2}{r_2^2}",
            "= 1",
            "&\\\\\\\\&",
            "\\begin{cases}",
            "    c = \\myvert{\\vec{p_0\\,f}}",
            "    = \\sqrt{r_1^2+r_2^2}",
            "    \\\\\\\\",
            "    f_1\\in\\mathbb{R}^2:f_1=(x_0-c,y_0)",
            "    \\\\\\\\",
            "    f_1\\in\\mathbb{R}^2:f_1=(x_0-c,y_0)",
            "\\end{cases}"
        ]
    },
    "Hiperboles": {
        "description": "Caracterizando Hiperboles",
        "prefix": "\\formula-hiperboles",
        "body": [
            "\\left(",
            "    \\frac{x-x_0}{r_1}",
            "\\right)",
            "- \\left(",
            "    \\frac{y-y_0}{r_2}",
            "\\right)",
            "= 1",
            "&\\\\\\\\&",
            "\\begin{cases}",
            "    c = \\myvert{\\vec{p_0\\,F}}",
            "    = \\sqrt{r_1^2+r_2^2}",
            "    \\\\\\\\",
            "    V=(x_0\\pm r_1,y_0)",
            "    \\\\\\\\",
            "    f=(x_0\\pm c,y_0)",
            "    \\\\\\\\",
            "    A = \\left{",
            "        (x,y):(y-y_0)",
            "        = \\pm\\frac{r_2}{r_1}",
            "        \\,(x-x_0)",
            "    \\right}",
            "\\end{cases}"
        ]
    },
    "Plano tangente a f": {
        "description": "Formula para plano tangente a uma superficie f genérica no ponto genérico (x0,y0,z0)",
        "prefix": "\\formula-plano-tangente",
        "body": [
            "\\left(",
            "    \\begin{aligned}",
            "        &",
            "               \\pdv{f}{x}(x_0)\\,(x-x_0)",
            "        &+\\\\\\\\+& \\pdv{f}{y}(y_0)\\,(y-y_0)",
            "        &+\\\\\\\\+& \\pdv{f}{z}(z_0)\\,(z-z_0)",
            "        &",
            "    \\end{aligned}",
            "\\right)",
            "= 0",
        ]
    },
    "Limite segundo cauchy": { 
        "description": "Definição de Limite segundo cauchy",
        "prefix": "\\formula-limite-cauchy",
        "body": [
            "\\forall\\,\\delta>0\\,",
            "\\exists\\,\\varepsilon>0:",
            "\\left(",
            "    \\left(",
            "        \\forall X\\in \\dominio",
            "        \\land",
            "        \\myVert{X-a}<\\varepsilon",
            "    \\right)",
            "    \\implies",
            "    \\myvert{",
            "        f(X)-b",
            "    }<\\delta",
            "\\right)",
        ]
    },
    "Jacobiana(2x2)": {
        "description": "Jacobiana 2x2",
        "prefix": "\\formula-Jacobiana-2x2",
        "body": [
            "\\begin{bmatrix}",
            "    \\pdv{x}{u} & \\pdv{x}{v}",
            "    \\\\\\\\",
            "    \\pdv{y}{u} & \\pdv{y}{v}",
            "\\end{bmatrix}_{(x,y)}"
        ]
    },
    "Matriz Hessiana 2x2": {
        "description": [
            "Matriz Hessiana 2x2\nmáximos e minimos\n",
            "det H:\n>0: Crítico Local\n=0: Indeterminável\n<0: Ponto de Sela\n",
            "det H>0 and d^2f/dx^2:\n<0: Mínimo Local\n>0: Máximo Local"
        ],
        "prefix": [
            "\\formula-matrix-hessiana-2",
            "\\formula-matriz-hessiana-2",
        ],
        "body": [
            "\\det H_f=\\begin{vmatrix}",
            "      \\pdv{f}{x,x}",
            "    & \\pdv{f}{x,y}",
            "    \\\\\\\\\\pdv{f}{y,x}",
            "    & \\pdv{f}{y,y}",
            "\\end{vmatrix}"
        ]
    },
    "Parametrização Coordenadas esfericas": {
        "Description": "Parametrização Coordenadas esfericas",
        "prefix": "\\formula-coordenadas-esfericas",
        "body": [
            "\\begin{cases}",
            "    x=r\\sin\\phi\\,\\cos\\theta",
            "    \\\\\\\\",
            "    y=r\\sin\\phi\\,\\sin\\theta",
            "    \\\\\\\\",
            "    z=\\cos\\phi",
            "    \\\\\\\\",
            "    \\myvert{\det J}=r^2\\sin\\phi",
            "\\end{cases}"
        ]
    },
    "Parametrização Coordenadas Cilindricas": {
        "Description": "Parametrização Coordenadas cilíndricas em R3",
        "prefix": "\\formula-coordenadas-cilindricas",
        "body": [
            "\\begin{cases}",
            "    x=r\cos\theta",
            "    \\\\\\\\",
            "    y=r\sin\theta",
            "    \\\\\\\\",
            "    z=z",
            "    \\\\\\\\",
            "    x^2+y^2=r",
            "    \\\\\\\\",
            "    \\tan{\\theta}=y/x",
            "    \\\\\\\\",
            "    \\myvert{det J}=r",
            "\\end{cases}"
        ]
    },
    /* -------------------------------------------------- */
    /*                        TEQB                        */
    /* -------------------------------------------------- */
    "Formula Gibbs": {
        "description": "Equação de Gibbs formad derivada",
        "prefix": "\\formula-gibbs-derivada",
        "body": [
            "\\odif{G}",
            "= V\\,\\odif{P}",
            "-S\\,\\odif{T}"
        ]
    },
    "Formula Variação de entropia de gás perfeito": {
        "description": "Equação para variação de entropia de um gás perfeito",
        "prefix": [
            "\\formula-entropia_gas_perfeito"
        ],
        "body": [
            "\\adif{S}_{g}",
            "=\\int_{T_1}^{T_2}{",
            "    n\\,C_{p,g}\\,\\frac{\\odif{T}}{T}",
            "}",
            "+ n\\,R\\,\\ln\\frac{P_1}{P_2}"
        ]
    },
    "Forumla de Clapeyron": {
        "description": "equação de clapeyron",
        "prefix": [
            "\\formula-Clapeyron"
        ],
        "body": [
            "\\odv{P}{T}",
            "= T^{-1}\\adv{H}{V}"
        ]
    },
    "Forumla de Clapeyron H vap": {
        "description": "equação de clapeyron para entalpia de vaporização",
        "prefix": [
            "\\formula-Clapeyron-H_vap"
        ],
        "body": [
            "\\ln\\frac{P_g}{P_l}",
            "= \\frac{P\\,\\adif{H}_{vap}}{R}",
            "\\,\\left(",
            "    \\frac{1}{T_l}",
            "    - \\frac{1}{T_g}",
            "\\right)"
        ]
    },
    /* -------------------------------------------------- */
    /*                        FT II                       */
    /* -------------------------------------------------- */
    "Difusão em misturas": {
        "description": "D_{A,M} Difusão de um gás em mistura",
        "prefix": "\\formula-Difusao-mistura",
        "body": [
            "\\mathscr{D}_{${1:X},M}",
            "= \\frac{",
            "    \\sum_{j=2}^{n}{y_{j}}",
            "}{",
            "    \\sum_{i=2}^{n}{y_i/\\mathscr{D}_{${1:X},i}}",
            "}$0",
        ]
    },
    "Variação da Difusão": {
        "description": "D ∝ T^{3/2}*Ω, D ∝ P^{-1}",
        "prefix": [
            "\\formula-Difusão-variacao",
            "\\formula-Difusão-prop"
        ],
        "body": [
            "\\frac",
            "{\\mathscr{D}_{${1:A},${2:B},${5:T_2}${7:\\,\\unit{\\kelvin}},${6:P_2}${8:\\,\\unit{\\atm}}}}",
            "{\\mathscr{D}_{${1:A},${2:B},${3:T_1}${7:\\,\\unit{\\kelvin}},${4:P_1}${8:\\,\\unit{\\atm}}}}",
            "= ",
            "\\,\\frac{${4:P_1}}{${6:P_2}}",
            "\\,\\left(",
            "    \\frac{${5:T_2}}{${3:T_1}}",
            "\\right)^{3/2}",
            "% \\,\\frac",
            "% {\\Omega_{\\mathscr{D},${3:T_1}}}",
            "% {\\Omega_{\\mathscr{D},${5:T_2}}}",
        ]
    },
    "Difusão em liquidos (Stokes-Einstein)": {
        "description": "𝒟 Liquidos",
        "prefix": "\\formula-difusao-liquido-stoke-einstein",
        "body": [
            "\\mathscr{D}_A",
            "=\\frac{k_B\\,T}{6\\,\\pi\\,\\mu\\,R_A}",
        ]
    },
    "Diluição infinita Hayduk-Laudie": {
        "description": "𝒟 inf em líquidos",
        "prefix": "\\formula-diluicao-infinita-hayduk-laudie",
        "body": [
            "\\mathscr{D}_{A,B}",
            "= 13.26*10^{-5}\\,\\mu_B^{-1.14}\\,V_A^{-0.589}"
        ]
    },
    "Equação de Scheibel": {
        "description": "𝒟",
        "prefix": "\\formula-difusao-sheibel",
        "body": [
            "\\frac{\\mathscr{D}_{A,B}\\,\\mu_B}{T}",
            "=\\frac{",
            "    8.2*10^{-8}",
            "    \\left(",
            "        1 + (3\\,V_B/V_A)^{2/3}",
            "    \\right)",
            "}{",
            "    V_A^{1/3}",
            "}",
        ]
    },
    "Fluxo": {
        "description": "Θ=1+N_B/N_A",
        "prefix": "\\formula-fluxo-geral",
        "body": [
            "N_{A,z}",
            "= -\\frac{c\\,D_{A,B}}{1-\\Theta\\,y_A}",
            "\\,\\odv{y_A}{z}",
        ]
    },
    "Fluxo estacionário plano": {
        "description": "S*N_{A,z}: cte;\n\nΘ=1+N_B/N_A",
        "prefix": "\\formula-fluxo-estacionario-plano",
        "body": [
            "N_{A,z}",
            "= \\frac{c\\,D_{A,B}}{\\Theta\\,\\adif{z}}",
            "\\,\\ln\\frac",
            "{1-\\Theta\\,y_{A,1}}",
            "{1-\\Theta\\,y_{A,0}}"
        ]
    },
    "Fluxo estacionário cilindro": {
        "description": "2*pi*r*L*N_{A,r}: cte;\n\nΘ=1+N_B/N_A",
        "prefix": "\\formula-fluxo-estacionario-cilindro",
        "body": [
            "N_{A,r}",
            "= -\\frac{c\\,D_{A,B}}{\\Theta\\,r_0\\,\\ln{(r_1/r_0)}}",
            "\\,\\ln\\frac",
            "{1-\\Theta\\,y_{A,1}}",
            "{1-\\Theta\\,y_{A,0}}"
        ]
    },
    "Fluxo estacionário esfera": {
        "description": "4*pi*r^2*N_{A,r}: cte;\n\nΘ=1+N_B/N_A",
        "prefix": "\\formula-fluxo-estacionario-esfera",
        "body": [
            "N_{A,r}",
            "= -\\frac{c\\,D_{A,B}}{\\Theta\\,r_0(1-r_0/r_1)}",
            "\\,\\ln\\frac",
            "{1-\\Theta\\,y_{A,1}}",
            "{1-\\Theta\\,y_{A,0}}"
        ]
    },
    /* -------------------------------------------------- */
    /*                        QF A                        */
    /* -------------------------------------------------- */
    "0th order Rate law derivated": {
        "description": "A -> P",
        "prefix": "\\formula-rate-deriv-0",
        "body": "\\odv{\\ch{[${1:A}]}}{t} = -k"
    },
    "0th order Rate law integrated": {
        "description": "A -> P",
        "prefix": "\\formula-rate-int-0",
        "body": "\\adif{\\ch{[A]}} = -k\\,\\adif{t}"
    },
    /* ==================== 1st order =================== */
    "1st order Rate law derivated": {
        "description": "A -> P",
        "prefix": "\\formula-rate-deriv-1",
        "body": "\\odv{\\ch{[${1:A}]}}{t} = -k\\,\\ch{[${1:A}]}"
    },
    "1st order Rate law integrated": {
        "description": "A -> P",
        "prefix": "\\formula-rate-int-1",
        "body": "\\adif{\\ln\\ch{[${1:A}]}}=-k\\,\\adif{t}"
    },
    "1st order pre Equilibrium A deriv": {
        "description": "A -> P\\n\\n[P]=[A]",
        "prefix": "\\formula-pre-equilibrium-deriv-A-1",
        "body": [
            "\\odv{\\ch{[A]}}{t}",
            "= \\ch{[A]}_0\\,k_{-1}",
            "- \\ch{[A]}\\,(k_{-1}+k_{+1})",
        ]
    },
    "1st order pre Equilibrium A": {
        "description": "A -> P\\n\\n[P]=[A]₀-[A]",
        "prefix": "\\formula-pre-equilibrium-A-1",
        "body": [
            "\\ch{[${1:A}]}",
            "=\\frac{",
            "   k_{-1}\\,\\ch{[A]}_0",
            "   + k_{+1}\\,\\exp(",
            "       -(k_{-1}+k_{+1})\\,t",
            "   )",
            "}{k_{-1}+k_{+1}}",
        ]
    },
    "1st order Equilibrium A": {
        "description": [
            "A -> P",
            "\n[P]=[A]₀-[A]"
        ],
        "prefix": "\\formula-equilibrium-A-1",
        "body": [
            "\\ch{[${1:A}]}",
            "=\\frac{k_{-1}\\,\\ch{[A]}_0}{k_{-1}+k_{+1}}",
        ]
    },
    "1st order Equilibrium P": {
        "description": [
            "A -> P",
            "\n[P]=[A]₀-[A]"
        ],
        "prefix": "\\formula-equilibrium-P-1",
        "body": [
            "\\ch{[${1:P}]}",
            "=\\frac{k_{+1}\\,\\ch{[A]}_0}{k_{-1}+k_{+1}}",
        ]
    },
    "1th order half-life": {
        "description": "t½",
        "prefix": "\\formula-half-life-1",
        "body": "t_{1/2} = \\ln{2}/k"
    },
    "1th order time constant": {
        "description": "te",
        "prefix": "\\formula-te-time-constant-1",
        "body": "t_{e} = k^{-1}"
    },
    /* ==================== 2nd order =================== */
    "2nd order Rate law derivated": {
        "description": "A² -> P",
        "prefix": "\\formula-rate-deriv-2.1",
        "body": "\\odv{\\ch{[${1:A}]}}{t} = -k\\,\\ch{[${1:A}]}^2"
    },
    "2nd order Rate law A² integrated": {
        "description": "A² -> P",
        "prefix": "\\formula-rate-int-2.1",
        "body": "\\adif{-\\ch{[${1:A}]}^{-1}} = -k\\,\\adif{t}"
    },
    "2nd order Rate law A+B integrated": {
        "description": "A + B -> P",
        "prefix": "\\formula-rate-int-2.2",
        "body": [
            "\\ln\\frac{",
            "   \\ch{[B]}/\\ch{[B]}_0",
            "}{",
            "   \\ch{[A]}/\\ch{[A]}_0",
            "}",
            "=\\frac{k\\,t}{\\ch{[B]}_0-\\ch{[A]}_0}",
        ]
    },
    "n-th order half-life": {
        "description": "t½",
        "prefix": "\\formula-half-life-n",
        "body": "t_{1/2} = \\left(k\\,\\ch{[${1:A}]_{0}^{n-1}}\\right)^{-1}"
    },
    /* ================ Activated complex =============== */
    "Arrhenius equation": {
        "description": "Relation between activation energy and reaction rate constant",
        "prefix": "\\formula-Arrhenius-equation",
        "body": "k=A\\,\\exp(-Ea/R\\,T)"
    },
    "Eyring equation": {
        "description": [
            "describe the relationship between reaction rate and temperature.",
            "\n Useful when we are talking about activation energy",
            "\n k‡: Transition state equilibrium constant",
            "\n ΔH‡: Activation entalpy",
        ],
        "prefix": [
            "\\formula-Eyring-equation",
            "\\formula-transition-state-equilibrium-constant"
        ],
        "body": [
            "\\ln{k^{\\ddagger}}",
            "= -\\frac{\\adif{H}^{\\ddagger}}{R\\,T}",
            "+ \\frac{\\adif{S}^{\\ddagger}}{R}"
        ]
    },
    "Arrhenius-Eyring equation": {
        "description": "Join both Arrhenius and Eyring equation to get a relation between Activation energy and entalpy of activantion",
        "prefix": [
            "\\formula-Arrhenius-Eyring",
            "\\formula-Activation-Energy-Entalpy"
        ],
        "body": "Ea=\\adif{H}^{\\ddagger} + R\\,T"
    },
    "Gibbs free energy equation": {
        "description": [
            "Gibbs free energy",
            "\nΔG: Change of Gibbs free energy of the reaction",
            "\nΔH: Change in entalpy",
            "\nΔS: Change in entropy",
        ],
        "prefix": "\\formula-gibbs-free-energy-equation",
        "body": "\\adif{G}=\\adif{H}-T\\,\\adif{S}"
    },
    "Gibbs free energy non standard state equation": {
        "description": [
            "Gibbs free energy when reaction is not in standard state",
            "\nStandard state: 1 atm, 298 K, pH 7",
            "\nΔG: Change of Gibbs free energy of the reaction",
            "\nΔG⁰: Standard Gibbs free energy",
            "\nK⁰: Equilibrium constant",
        ],
        "prefix": "\\formula-gibbs-free-energy-non-standard-equation",
        "body": "\\adif{G}=\\adif{G}^{0}+R\\,T\\,\\ln{K}"
    },
    "Gibbs free energy of activation": {
        "description": [
            "Transition theory, Gibbs free energy of activation",
            "\nStandard state: 1 atm, 298 K, pH 7",
            "\nΔG‡: Gibbs energy of activation",
            "\nΔH‡: Entalpy of activation",
            "\nΔS‡: Entropy of activation",
        ],
        "prefix": "\\formula-gibbs-free-energy-activation-equation",
        "body": [
            "\\adif{G}^{\\ddagger}=",
            "= -R\\,T\\,\\ln{K}",
            "= \\adif{H}^{\\ddagger}-T\\,\\adif{S}^{\\ddagger}"
        ]
    },
