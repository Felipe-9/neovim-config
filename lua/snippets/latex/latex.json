            "    -- Print specific files by number",
            "    index = {1}",
            "    -- % ---------------------------------------------------- %",
            "    -- %                         Loop                         %",
            "    -- % ---------------------------------------------------- %",
            "    -- for i=firstFile,lastFile,1 -- Print range",
            "    -- for v,i in ipairs(index)   -- Print specific",
            "    for i=firstFile,lastFile,1 -- Print range",
            "    do",
            "        tex.print(string.split(table.concat{",
            "        [[",
            "            \\subfile{]], dir, prefix, string.format(\"%d\", i), sufix, [[}\\newpage",
            "        ]]",
            "        },\"\\n\"))",
            "    end",
            "    -- % ==================================================== %",
            "\\end{luacode*}", 
            "",
            "",
            "\\end{document}"
        ]
    }
